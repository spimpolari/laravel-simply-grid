{
    "name": "spimpolari/laravel-simply-grid",
    "description": "Easy Datatables Grid",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "spimpolari",
            "email": "spimpolari@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "spimpolari\\LaravelSimplyGrid\\": "src"
        },
        "files": [
